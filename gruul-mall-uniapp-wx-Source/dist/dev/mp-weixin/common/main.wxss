@charset "UTF-8";
page {
  min-height: 100%;
  background: #f1f2f6;
}
view {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/*按钮*/
button {
  line-height: inherit;
  border-radius: inherit;
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  font-size: inherit;
  opacity: 1;
  background-color: inherit;
}
button::after {
    border: 0;
}
.m__button {
  min-width: 140rpx;
  padding: 6rpx 0;
  text-align: center;
  border-radius: 30rpx;
  border: 2rpx solid #989898;
  outline: none;
  background: #fff;
  margin-left: 20rpx;
}
.m__button.primary {
    background: #fe4e63;
    border-color: #fe4e63;
    color: #fff;
}
.m__button.primary[disabled] {
      border-color: #fda7b2;
      background: #fda7b2;
      color: #fff3f6;
}
.text__pink {
  color: #fe4e63;
}
.text__green {
  color: #08bb07;
}
.text__primary {
  color: #000000;
}
